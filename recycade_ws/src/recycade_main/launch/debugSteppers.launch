<launch>

    <!--debug node-->
    <node name="debug_node" pkg = "recycade_main" type = "recycade_main_stepper_debug" output = "screen"/>

    <!--rosserial node-->
    <node pkg="rosserial_python" type="serial_node.py" name="serial_node">
        <param name="port" value="/dev/ttyACM0"/>
        <param name="baud" value="115200"/>
    </node>
</launch>